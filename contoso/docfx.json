{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "legacy",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "non-version/**"
        ],
        "src": "legacy"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "active",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "non-version/**"
        ],
        "src": "active"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "src": "non-version",
        "dest": "non-v"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "_themes/**",
          "**/obj/**",
          "**/includes/**",
          "non-version/**"
        ],
        "src": "legacy",
        "group": "legacy"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "non-version/**"
        ],
        "src": "active",
        "group": "active"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "non-version",
        "dest": "non-v"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "titleSuffix": "Contoso", 
      "uhfHeaderId": "MSDocsHeader-DotNet", 
      "extendBreadcrumb": true 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "contoso",
    "exportRawModel": true,
    "groups": {
      "legacy": {
        "dest": "legacy",
        "moniker_range": "< contososerver-2.0"
      }, 
      "active": {
        "dest": "active",
        "moniker_range": ">= contososerver-2.0 || >= contosoclient-1.0"
      }
    }
  }
}
